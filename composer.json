{
    "name": "jesusgarciaespinosa/crud-generator",
    "license": "MIT",
    "description": "Lumen CRUD Generator",
    "keywords": [
        "lumen",
        "crud",
        "crud generator",
        "lumen crud generator",
        "api generator"
    ],
    "authors": [
        {
            "name": "Jesus Garcia Espinosa",
            "email": "jesus.garcia.espinosa@gmail.com",
            "homepage": "https://github.com/jesusgarciaespinosa"
        }
    ],
    "require": {
        "php": ">=5.6.4|>=7.0",
        "illuminate/support": "^5.3|^6.0",
        "laravelcollective/html": "^5.3|^6.0",
        "laravel/lumen-framework": "^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.6",
        "phpunit/phpunit": "^5.7",
        "orchestra/testbench": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Jge\\CrudGenerator\\": "src/"
        },
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jge\\CrudGenerator\\CrudGeneratorServiceProvider"
            ]
        }
    }
}
